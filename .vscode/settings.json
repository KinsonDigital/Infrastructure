{
	"window.title": "ðŸ¦•${activeRepositoryName} - [${activeRepositoryBranchName}]",
	"workbench.colorCustomizations": {
		"titleBar.activeBackground": "#21351f"
	},
    "cSpell.words": [
		"cicd",
		"Creds",
		"endgroup",
		"flatcontainer",
		"kdadmin",
		"Kinson",
		"mediumseagreen",
